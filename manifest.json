{
    "manifest_version": 3,
    "name": "URL Decoder",
    "version": "1.0",
    "description": "Decode and open URLs with a right-click",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}